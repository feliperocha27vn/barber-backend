# Porta do servidor
PORT=3333

# URL de conexão com o banco de dados PostgreSQL
# Formato: postgresql://[usuario]:[senha]@[host]:[porta]/[nome_do_banco]?schema=public
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/barbershop?schema=public"

# Secret para geração de tokens JWT (use uma string aleatória e segura em produção)
JWT_SECRET="your-super-secret-jwt-key-here"